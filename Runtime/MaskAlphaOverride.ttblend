#pragma HLSL_Version 2018
#pragma ColorSpace Gamma
#pragma Key "RenaSAdditionalBlending/MaskAlphaOverride"
#pragma KeyName_jp "RenaSAdditionalBlending/マスクアルファオーバーライド"


float4 ColorBlend(float4 BaseColor, float4 AddColor)
{
    float3 Bcol = BaseColor.rgb;
    return float4(Bcol, saturate(1 - AddColor.a));
}
